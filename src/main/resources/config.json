{
  "custom_class_for_sign_output": {
    "enable": false,
    "compile": false
  },
  "enable_back_teleport": true,
  "commands": {
    "clipboard": {
      "name": "clipboard",
      "clip_limit": 5000
    },
    "signs": {
      "name": "signs",
      "flip": true
    },
    "edit": {
      "name": "edit"
    }
  },
  "command_buffer_cooldown": 700,
  "short-commands": {
    "enable": true,
    "constructor": {
      "n": {
        "arguments": {
          "numbers": {
            "type": "greedy",
            "suggestions": [],
            "parser": "double",
            "split": " "
          }
        },
        "structure": ["numbers"],
        "run": "num ${1}"
      },
      "t": {
        "arguments": {
          "text": {
            "type": "greedy",
            "suggestions": []
          }
        },
        "structure": ["text"],
        "run": "txt ${1}"
      },
      "vl": {
        "arguments": {
          "name": {
            "type": "greedy",
            "suggestions": ["@history.local"],
            "split": " "
          }
        },
        "structure": ["name"],
        "run": "var local ${1}"
      },
      "vg": {
        "arguments": {
          "name": {
            "type": "greedy",
            "suggestions": ["@history.game"],
            "split": " "
          }
        },
        "structure": ["name"],
        "run": "var ${1}"
      },
      "vs": {
        "arguments": {
          "name": {
            "type": "greedy",
            "suggestions": ["@history.save"],
            "split": " "
          }
        },
        "structure": ["name"],
        "run": "var save ${1}"
      }
    }
  }
}